{
  "name": "@base/analytics",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.tsx"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    },
    "./posthog/client": {
      "types": "./dist/posthog/client.d.ts",
      "default": "./src/posthog/client.tsx"
    },
    "./posthog/server": {
      "types": "./dist/posthog/server.d.ts",
      "default": "./src/posthog/server.ts"
    }
  },
  "scripts": {
    "dev": "tsgo --watch",
    "build": "tsc && tsc-alias",
    "type-check": "tsgo --noEmit --emitDeclarationOnly false",
    "clean": "git clean -xdf .cache .turbo dist node_modules"
  },
  "dependencies": {
    "@base/api": "workspace:^",
    "@base/ui": "workspace:^",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "next": "^15.3.3",
    "posthog-js": "^1.249.4",
    "posthog-node": "^4.18.0"
  },
  "devDependencies": {
    "@base/typescript": "workspace:^",
    "tsc-alias": "^1.8.16"
  }
}
