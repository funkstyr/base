    "@base/auth": "workspace:^",
    "@base/db": "workspace:^",
    "@base/supabase": "workspace:^",
    "@orpc/arktype": "^1.4.5",
    "@orpc/client": "^1.4.5",
    "@orpc/openapi": "^1.4.5",
    "@orpc/server": "^1.4.5",
    "ai": "^4.3.16",
    "arktype": "^2.1.20",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "hono": "^4.7.11"
  },
  "devDependencies": {
    "@base/typescript": "workspace:",
    "tsc-alias": "^1.8.16"
  }
}
