    "@atproto/oauth-client-node": "^0.2.22",
    "@atproto/sync": "^0.1.24",
    "@atproto/syntax": "^0.3.4",
    "@atproto/xrpc-server": "^0.7.19",
    "arktype": "^2.1.20",
    "hono": "^4.7.11"
  },
  "devDependencies": {
    "@atproto/lex-cli": "^0.4.1",
    "@base/typescript": "workspace:",
    "@types/bun": "^1.2.15",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3"
  }
